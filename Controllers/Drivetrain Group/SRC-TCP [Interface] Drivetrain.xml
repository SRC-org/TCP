if abs(current) &gt; abs(target)+0.5 then
	thr=0
	multI=3
else
	thr=thrTemp
	multI=1
end


sn(1,thr)
sn(2,rpsd)
sn(3,a)
sn(4,targetTemp)
sn(5,tinteg)
